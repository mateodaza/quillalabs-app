{
  "version": 2,
  "alias": "quillalabs.com",
  "name": "quillalabs-app",
  "builds": [
      { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/event/(?<name>[^/]+)$", "dest": "/event?name=$name" },
    { "src": "/checkout/(?<event>[^/]+)$", "dest": "/checkout?event=$event" }
  ]
}